{
  "name": "nei-ui",
  "version": "0.0.2",
  "description": "呐呐呐呐呐呐",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "author": "bbbiaozzza",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 2333",
    "build": "webpack --config webpack.config.js --mode=production",
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src/components --out-dir lib/ --ignore **/__tests__,**/__mocks__",
    "build:esm": "babel src/components --out-dir esm/ --ignore **/__tests__,**/__mocks__",
    "copy-css:esm": "cpx \"src/components/**/*.css\" esm",
    "copy-css:lib": "cpx \"src/components/**/*.css\" lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naichazhenhaohe/Nei.git"
  },
  "bugs": {
    "url": "https://github.com/naichazhenhaohe/Nei/issues"
  },
  "files": [
    "esm",
    "lib"
  ],
  "homepage": "https://github.com/naichazhenhaohe/Nei#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/react": "^5.3.17",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "style-loader": "^1.1.3",
    "styled-jsx-plugin-sass": "^1.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "less": "^3.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-jsx": "^3.2.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-object-rest-spread",
      [
        "@babel/proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "styled-jsx-plugin-sass"
          ]
        }
      ]
    ]
  }
}
